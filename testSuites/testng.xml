<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="All Test Suite">
    <listeners>
        <listener class-name="BaseTestComponents.Listeners"/>
    </listeners>
    <test thread-count="5" verbose="2" preserve-order="true" name="Positive Tests">
        <classes>
            <class name="Tests.PositiveTests"/>
        </classes>
    </test>
    <test thread-count="5" verbose="2" preserve-order="true" name="Negative Tests">
        <classes>
            <class name="Tests.ErrorTests"/>
        </classes>
    </test>
</suite>